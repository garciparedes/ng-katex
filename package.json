{
  "name": "ng-katex",
  "version": "1.0.0",
  "description": "Angular module to write beautiful math expressions with TeX syntax boosted by KaTeX library",
  "main": "ng-katex.umd.js",
  "jsnext:main": "ng-katex.esm.js",
  "module": "ng-katex.esm.js",
  "types": "ng-katex.d.ts",
  "scripts": {
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc && ngc",
    "postbundle": "bash tools/file-remover.sh",
    "build": "npm run bundle && node tools/cleaner.js",
    "publish": "cd ./dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garciparedes/ng-katex.git"
  },
  "author": {
    "name": "Sergio GarcÃ­a Prado",
    "email": "sergio@garciparedes.me",
    "url": "https://garciapredes.me"
  },
  "keywords": [
    "angular",
    "katex",
    "latex",
    "math",
    "package",
    "typescript",
    "angular2",
    "typesetting",
    "formulae",
    "library",
    "module",
    "angular-module",
    "angular-directive",
    "angular-component"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garciparedes/ng-katex/issues"
  },
  "homepage": "https://github.com/garciparedes/ng-katex#readme",
  "dependencies": {
    "katex": "^0.8.3"
  },
  "peerDependencies": {
    "@angular/core": ">=2.2.1"
  },
  "devDependencies": {
    "@angular/compiler": "4.4.6",
    "@angular/compiler-cli": "4.4.6",
    "@angular/core": "4.4.6",
    "clean-css": "4.1.9",
    "node-sass": "4.7.2",
    "rimraf": "2.6.2",
    "rollup": "0.55.1",
    "rollup-plugin-angular": "0.5.3",
    "rollup-plugin-copy": "0.2.3",
    "rollup-plugin-typescript2": "0.10.0",
    "rollup-plugin-uglify": "3.0.0",
    "rxjs": "5.5.6",
    "typescript": "2.6.2",
    "zone.js": "0.8.20"
  }
}
